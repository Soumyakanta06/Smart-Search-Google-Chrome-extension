{
    "manifest_version": 2,
    "name": "Word Scout",
    "description": "Quick Search meaning of words just by double click on the specified word",
    "version": "2.0",
    "icons": {"128": "icon-128px.png"},
    "browser_action" : {
        "default_icon": "icon-128px.png"
    
},
"content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }
]
}
